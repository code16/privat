{
  "name": "code16/privat",
  "description": "Simple private access package for Laravel.",
  "authors": [{
    "name": "Philippe Lonchampt",
    "email": "philippe@code16.fr"
  }],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": ">=5.5.9",
    "laravel/framework": "5.2.*|5.3.*|5.4.*|5.5.*|5.6.*"
  },
  "require-dev": {
    "laravel/laravel": "5.2.*|5.3.*|5.4.*|5.5.*|5.6.*",
    "mockery/mockery": "0.9.*",
    "phpunit/phpunit": "~5.0",
    "symfony/css-selector": "2.8.*|3.0.*",
    "symfony/dom-crawler": "2.8.*|3.0.*",
    "laravel/browser-kit-testing": "~1.0"
  },
  "autoload": {
    "psr-4": {
      "Code16\\Privat\\": "src/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/TestCase.php"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "Code16\\Privat\\PrivatServiceProvider"
      ]
    }
  }
}
