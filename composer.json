{
  "name": "code16/privat",
  "description": "Simple private access package for Laravel.",
  "authors": [{
    "name": "Philippe Lonchampt",
    "email": "philippe@code16.fr"
  }],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": "8.4.*",
    "laravel/framework": "^11.0|^12.0"
  },
  "require-dev": {
    "fakerphp/faker": "^1.19.0",
    "mockery/mockery": "^1.5.0",
    "pestphp/pest": "^3.0",
    "pestphp/pest-plugin-laravel": "^3.0",
    "phpunit/phpunit": "^11.0",
    "doctrine/dbal": "^3.5",
    "orchestra/testbench": "9.*|10.*",
    "pestphp/pest-plugin-drift": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "Code16\\Privat\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Code16\\Privat\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Code16\\Privat\\PrivatServiceProvider"
      ]
    }
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  }
}
