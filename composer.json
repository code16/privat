{
  "name": "code16/privat",
  "description": "Simple private access package for Laravel.",
  "authors": [{
    "name": "Philippe Lonchampt",
    "email": "philippe@code16.fr"
  }],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": "^7.4|^8.1|^8.2",
    "laravel/framework": "5.5.*|5.6.*|5.7.*|5.8.*|^6.0|^7.0|^8.0|^9.0|^10.0",
    "laravel/legacy-factories": "^1.1"
  },
  "require-dev": {
    "fakerphp/faker": "^1.19.0",
    "mockery/mockery": "0.9.*|^1.0",
    "phpunit/phpunit": "~6.0|~7.0|~8.0|~8.5|~9.0|~10.0",
    "doctrine/dbal": "^2.5|^3.0",
    "orchestra/testbench": "~3.5|~3.6|~3.7|~3.8|4.*|5.*|6.*|7.*|8.*"
  },
  "autoload": {
    "psr-4": {
      "Code16\\Privat\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Code16\\Privat\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Code16\\Privat\\PrivatServiceProvider"
      ]
    }
  }
}
