{
    "name": "code16/privat",
    "description": "Simple private access package for Laravel.",
    "authors": [
        {
            "name": "Philippe Lonchampt",
            "email": "philippe@code16.fr"
        }
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^8.1|^8.2",
        "laravel/framework": "^9.0|^10.0|^11.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.19.0",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "~9.0|~10.0",
        "doctrine/dbal": "^3.0",
        "orchestra/testbench": "7.*|8.*|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Code16\\Privat\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Code16\\Privat\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Code16\\Privat\\PrivatServiceProvider"
            ]
        }
    }
}
