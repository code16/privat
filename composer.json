{
  "name": "code16/privat",
  "description": "Simple private access package for Laravel.",
  "authors": [{
    "name": "Philippe Lonchampt",
    "email": "philippe@code16.fr"
  }],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": ">=7.1.3",
    "laravel/framework": "5.5.*|5.6.*|5.7.*|5.8.*|6.0|6.1|6.2|6.3|6.4"

  },
  "require-dev": {
    "fzaninotto/faker": "~1.4",
    "mockery/mockery": "0.9.*|1.0.*",
    "phpunit/phpunit": "~6.0|~7.0|~8.0",
    "doctrine/dbal": "~2.5",
    "orchestra/testbench": "~3.5|~3.6|~3.7|~3.8|4.*"
  },
  "autoload": {
    "psr-4": {
      "Code16\\Privat\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Code16\\Privat\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Code16\\Privat\\PrivatServiceProvider"
      ]
    }
  }
}
